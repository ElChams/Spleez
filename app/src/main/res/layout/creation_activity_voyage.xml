<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="com.slpeez.spleez.CreerParticiper_Activity"
    android:name="com.google.android.gms.maps.SupportMapFragment">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:background="@drawable/valley"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true" />

    <ImageView
        android:layout_width="350dp"
        android:layout_height="400dp"
        android:id="@+id/imageView10"
        android:background="#6e3b4656"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/linearLayout2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />


    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPostalAddress"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_below="@+id/activityname"
        android:layout_toStartOf="@+id/valider" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_marginTop="130dp"
        android:layout_below="@+id/editText2"
        android:layout_alignStart="@+id/editText6" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_below="@+id/editText3"
        android:layout_alignStart="@+id/editText3"
        android:layout_alignEnd="@+id/editText3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ou ?"
        android:textColor="#FFFFFFFF"
        android:id="@+id/adresse"
        android:layout_below="@+id/textView31"
        android:layout_alignEnd="@+id/textView5" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:textColor="#FFFFFFFF"
        android:id="@+id/activityname"
        android:layout_below="@+id/textView16"
        android:layout_alignStart="@+id/editText2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Quand ?"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView4"
        android:layout_alignBaseline="@+id/editText4"
        android:layout_alignBottom="@+id/editText4"
        android:layout_alignStart="@+id/textView33" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="L&apos;heure ?"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView5"
        android:layout_marginBottom="10dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Combien ?"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView6"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignEnd="@+id/adresse" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/annuler"
        android:background="@drawable/annuler"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/textView5" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/valider"
        android:background="@drawable/valider"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <fragment xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:map="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        tools:context="com.slpeez.spleez.Creation_activity.Creation_Activity_Voyage"
        android:layout_below="@+id/editText2"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/textView33" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/editText6"
        android:background="#a6000000"
        android:layout_below="@+id/editText4"
        android:layout_alignParentEnd="true"
        android:layout_alignStart="@+id/editText2"
        android:layout_above="@+id/annuler" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Commentaire"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView18"
        android:layout_alignTop="@+id/editText6"
        android:layout_alignParentStart="true"
        android:layout_marginTop="47dp" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/barmenu"
        android:id="@+id/linearLayout2"
        android:weightSum="1"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/spleez"
            android:background="@drawable/logo"
            android:layout_marginLeft="30dp" />

        <ImageButton
            android:layout_width="29dp"
            android:layout_height="wrap_content"
            android:id="@+id/logoamis"
            android:background="@drawable/logoamis"
            android:contentDescription=""
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/imageView8"
            android:layout_marginTop="4dp"
            android:layout_marginLeft="60dp" />

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/logoevent"
            android:background="@drawable/logoevenement"
            android:layout_marginLeft="60dp"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/noteuser"
            android:layout_marginTop="4dp" />
    </LinearLayout>

    <ImageView
        android:layout_width="160dp"
        android:layout_height="7dp"
        android:id="@+id/imageView9"
        android:background="@drawable/barselectmenu"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/linearLayout2" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/buttonprofil"
        android:background="@drawable/ptofilicone"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Nom de l&apos;activité"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView31"
        android:layout_alignBaseline="@+id/activityname"
        android:layout_alignBottom="@+id/activityname"
        android:layout_alignEnd="@+id/adresse" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Durée"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView33"
        android:layout_alignTop="@+id/editText3"
        android:layout_alignEnd="@+id/adresse" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="VOYAGE"
        android:textColor="#FFFFFFFF"
        android:id="@+id/textView16"
        android:layout_below="@+id/buttonprofil"
        android:layout_toStartOf="@+id/imageView9" />

</RelativeLayout>
